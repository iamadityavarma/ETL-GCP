[run]
# Coverage configuration

source = .
omit =
    */tests/*
    */venv/*
    */site-packages/*
    */__pycache__/*
    */migrations/*
    setup.py

[report]
# Reporting options
precision = 2
show_missing = True
skip_covered = False

# Fail if coverage is below threshold
fail_under = 70

exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod

[html]
directory = htmlcov
